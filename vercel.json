{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "installCommand": "npm install",

  "framework": null,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
        {
        "source": "/(.*)",
        "headers": [
            {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
            }
        ]
        }
    ]
}