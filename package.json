{
  "name": "plinkk",
  "version": "1.25.07.31",
  "description": "Template moderne pour créer votre page de liens personnalisée avec TypeScript",
  "main": "index.html",
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist public",
    "build": "tsc && npm run copy-files",
    "copy-files": "mkdir -p public && cp -r contents public/ && cp index.html public/ && cp -r contents/images public/contents/"
  },
  "keywords": [
    "linktree",
    "links",
    "template",
    "typescript",
    "discord",
    "profile",
    "portfolio"
  ],
  "author": "Klaynight Dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Klaynight-dev/plinkk.git"
  },
  "bugs": {
    "url": "https://github.com/Klaynight-dev/plinkk/issues"
  },
  "homepage": "https://github.com/Klaynight-dev/plinkk#readme",
  "devDependencies": {
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "private": true,
  "dependencies": {
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  }
}
