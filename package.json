{
  "name": "plinkk-links-template",
  "version": "1.25.07.31",
  "description": "Template moderne pour créer votre page de liens personnalisée avec TypeScript",
  "main": "index.html",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "dev": "tsc --watch",
    "clean": "rimraf public",
    "copy-assets": "npm run clean && mkdir public && cp -r contents public/ && cp index.html public/",
    "serve": "python -m http.server 8000 --directory public",
    "start": "npm run build && npm run serve",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "linktree",
    "links",
    "template",
    "typescript",
    "discord",
    "profile",
    "portfolio"
  ],
  "author": "Klaynight Dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Klaynight-dev/links_website_template.git"
  },
  "bugs": {
    "url": "https://github.com/Klaynight-dev/links_website_template/issues"
  },
  "homepage": "https://github.com/Klaynight-dev/links_website_template#readme",
  "devDependencies": {
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
